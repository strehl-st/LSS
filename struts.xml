<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
"http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
<package name="strutsBean" extends="struts-default" namespace="/">
<action name="register" class="userAction">
	<result name="success">/regSuccess.jsp</result>
	<result name="fail">/regFail.jsp</result>
</action>
<action name="login" class="loginAction">
	<result name="success">/logSuccess.jsp</result>
	<result name="fail">/loginFail.jsp</result>
	<result name="admin">/admin.jsp</result>
</action>
<action name="orderSubmit" class="orderSubmit">
	<result name="success">/login.jsp</result>
	<result name="fail">/loginFail.jsp</result>
</action>
<action name="allOrders" class="checkorderAction">
		<result name="success">/logSuccess.jsp</result>
</action>
</package>
</struts>